  "name": "StereoJukebox",
  "description": "Makes music discs play in both ears instead of depending on position.",
  "authors": [],
  "contact": {},
  "license": "GPL-3.0",
  "icon": "assets/stereojukebox/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "net.zbound.stereojukebox.client.StereoJukeboxDataGenerator"
    ],
    "client": [
